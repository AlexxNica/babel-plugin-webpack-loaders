{
  "name": "webpack-for-babel-plugin",
  "version": "0.0.1",
  "description": "webpack plugin for babel 6",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "lint": "eslint src test examples",
    "_plugin": "npm run clean && npm run build && babel --plugins ./lib/plugin.js ./examples/tmp.js",
    "plugin": "npm run clean && npm run build && NODE_ENV=TEST babel ./examples/tmp.js",
    "plugin-run": "npm run clean && npm run build && NODE_ENV=TEST babel-node ./examples/tmp.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.3.26",
    "babel-register": "^6.3.13",
    "babylon": "^6.3.26",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.13.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7"
  }
}
